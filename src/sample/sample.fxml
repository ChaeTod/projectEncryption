<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="200.0" prefWidth="600.0">
         <children>
            <Button fx:id="opnBtn" layoutX="63.0" layoutY="56.0" mnemonicParsing="false" onAction="#setFile" prefHeight="25.0" prefWidth="112.0" text="Open File" />
            <Label layoutX="66.0" layoutY="23.0" text="Select the file">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="keyfield" layoutX="239.0" layoutY="56.0" onAction="#getKey" prefHeight="25.0" prefWidth="90.0" />
            <Label layoutX="264.0" layoutY="19.0" text="KEY">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="wrtBtn" layoutX="93.0" layoutY="129.0" mnemonicParsing="false" onAction="#writeFile" text="Write" />
         </children></Pane>
   </children>
</GridPane>
